    <block  wx:for="{{subList}}" wx:key="{{subIndex}}" wx:for-item="sub" wx:for-index="subIndex">
      <view>
        <text class='dykctext'>{{sub.name}}</text>
        <text class='genduotuijian' data-subid="{{sub.id}}" catchtap='moreSubCourse'>查看更多</text>
      </view>
      <view class="dykcsp">
        <block wx:for="{{sub.courseList}}" wx:key="index" wx:for-item="course" wx:for-index="index">

            <block wx:if="{{subIndex==1&&index==0}}">
              <image data-id="{{course.id}}" data-index='{{index}}'  class="ddwhjy_img" src='{{course.thumbnail}}' catchtap='bofang' src='{{course.thumbnail}}'></image>
              <view class='ddwhjy_jianju'>
                <text class='ddwjjy_bt'>{{course.name}}</text>
                <view class='ddjy_xuanke'>
                  <block wx:if="{{course.isSign==0}}">
                    <text data-id="{{course.id}}" data-subIndex="{{subIndex}}" data-index="{{index}}" class='xk' catchtap='chooseCourse'>选课</text>
                  </block>
                  <block wx:else>
                    <text data-id="{{course.id}}" class='yixuan' catchtap='moreCourseTap'>已选</text>
                  </block>
                  
                </view>
              </view>
            </block>

            <view  class='left'  wx:else>
              <image data-id="{{course.id}}" class='dykctupian' data-index='{{index}}' catchtap='bofang' src='{{course.thumbnail}}'></image>
              <!-- <image class='dykctupian' data-index='{{index}}' src='/img/yiyuan.png'></image> -->
              <text class="xueshi">学时:{{course.hours}}</text>
              <text class="shijian">{{courseTime}}</text>
              <text class="shuoming">{{course.name}}</text>
              <view class='zcfgjy_xz'>
                <block wx:if="{{course.isSign==0}}">
                  <text data-id="{{course.id}}" data-subIndex="{{subIndex}}" data-index="{{index}}" class='xk' catchtap='chooseCourse'>选课</text>
                </block>
                <block wx:else>
                  <text data-id="{{course.id}}" class='yixuan' catchtap='moreCourseTap'>已选</text>
                </block>
              </view>
            </view>

        </block>  
        <view style='clear:both;'></view>
      </view>  
    </block>

