<!--课程库页面 -->
<view class='kck'>
  <view class='dingwei'>
    <view class='tit'>
      <image class='img' src="/img/logo.png" bindtap="onCancelImgTap"></image>
      <text class='xuanke yanse'>必修课</text>
      <text class='xuanxiuke'>选修课</text>
    </view>
    <view class='tit2'>
      <image class="sousuo" src='/img/sousuokuang.png' bindtap='onSousuo'></image>
      <text class='sp xzzhangtai' catchtap='shipin'>视频</text>
      <text class='sp' catchtap='tuwen'>图文</text>
      <text class='sp' catchtap='yuyin'>语音</text>
    </view>
  </view>
  <view class='neirong'>
    <!--视频内容 -->
    <view class='shipineirong'  wx:if="{{shipinShow}}" >
      <view class='lskcnr'>
        <swiper indicator-dots="true" autoplay="true" interval="3000" duration="500" circular="true" 
          current="{{swiperCurrent}}"  class="swiper lunbo" previous-margin="30px" next-margin="30px">
          <block wx:for="{{imgUrls}}"  wx:for-item="course" wx:for-index="index" wx:key="index">
            <swiper-item class="titimg"> 
              <image  data-index='{{index}}' data-courseid='{{course.id}}' src="{{course.thumbnail}}"
                class="biaotou" bindtap="bofang" />
              <text class="titxinxi">{{course.name}}</text>
            </swiper-item>
          </block> 
        </swiper>
      </view>
      <!--正在热播 -->
      <!-- <view class='lishikc' catchtap="tzbfy">
